{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "type": {},
    "test": {},
    "storybook": {
      "cache": true
    },
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    }
  },
  "baseBranch": "origin/main"
}
